{
  "name": "cli",
  "private": true,
  "version": "0.1.0",
  "description": "Qwery Workspace command line interface",
  "bin": {
    "qwery": "dist/index.js"
  },
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "tsup --config tsup.config.ts && chmod +x dist/index.js",
    "clean": "rimraf dist node_modules/.cache/tsbuildinfo.json",
    "dev": "echo \"Use pnpm --filter cli start -- <command>\"",
    "format": "prettier --check \"src/**/*.{ts,tsx}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint src",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix src",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "vitest run --logHeapUsage --coverage --silent",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^3.0.56",
    "@ai-sdk/azure": "^2.0.73",
    "@ai-sdk/openai": "^2.0.68",
    "@ai-sdk/provider": "^2.0.0",
    "@duckdb/node-api": "1.4.2-r.1",
    "@langchain/core": "^0.3.0",
    "@mlc-ai/web-llm": "^0.2.79",
    "@qwery/agent-factory-sdk": "workspace:*",
    "@qwery/domain": "workspace:*",
    "@qwery/extensions-sdk": "workspace:*",
    "@qwery/repository-in-memory": "workspace:*",
    "ai": "^5.0.93",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "nanoid": "^5.1.6",
    "pg": "^8.13.1",
    "react": "catalog:",
    "react-dom": "catalog:",
    "uuid": "^11.0.3",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@qwery/eslint-config": "workspace:*",
    "@qwery/prettier-config": "workspace:*",
    "@qwery/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "@types/pg": "^8.11.10",
    "@vitest/coverage-v8": "4.0.7",
    "eslint": "^9.39.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.1",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "prettier": "@qwery/prettier-config"
}
