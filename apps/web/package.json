    "@react-router/node": "^7.9.5",
    "@react-router/serve": "^7.9.5",
    "@tanstack/react-query": "5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-table": "^8.21.3",
